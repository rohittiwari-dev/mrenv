{
	"name": "mrenv",
	"version": "1.0.0",
	"description": "Enhanced type-safe environment variable management system",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"bin": {
		"mrenv": "./dist/cli.cjs"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsup",
		"typecheck": "tsc --noEmit",
		"lint": "eslint src",
		"prepublishOnly": "npm run typecheck && npm run lint && npm run build",
		"test-cli": "node scripts/cli.js"
	},
	"keywords": [
		"environment",
		"variables",
		"env",
		"dotenv",
		"zod",
		"typescript",
		"type-safe",
		"validation",
		"t3-env",
		"env-variables",
		"next-js",
		"react",
		"node"
	],
	"author": "Your Name",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/mrenv.git"
	},
	"bugs": {
		"url": "https://github.com/yourusername/mrenv/issues"
	},
	"homepage": "https://github.com/yourusername/mrenv#readme",
	"devDependencies": {
		"@types/node": "^20.11.24",
		"chalk": "^5.4.1",
		"commander": "^13.1.0",
		"inquirer": "^12.4.3",
		"source-map-support": "^0.5.21",
		"tsup": "^8.0.2",
		"typescript": "^5.3.3",
		"zod": "^3.22.0"
	},
	"peerDependencies": {
		"zod": "^3.22.0"
	},
	"engines": {
		"node": ">=16.0.0"
	}
}
